{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "interface-name": false,
    "jsx-boolean-value": false
  },
  "linterOptions": {
    "exclude": [
      "./src/serviceWorker.ts"
    ]
  }
}
